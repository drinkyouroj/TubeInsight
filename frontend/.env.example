# Supabase Project URL - Find this in your Supabase project's API settings
NEXT_PUBLIC_SUPABASE_URL=

# Supabase Anon Key (public) - Find this in your Supabase project's API settings
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# URL for your Flask backend API
# For local development, this will typically be http://localhost:PORT/api
# (e.g., http://localhost:5000/api if your Flask app runs on port 5000)
NEXT_PUBLIC_BACKEND_API_URL=http://localhost:5000/api

# Add any other frontend-specific public environment variables here
NEXT_PUBLIC_SITE_URL=http://localhost:3000 # this changes when moved to production